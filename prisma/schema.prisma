// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_APP_URL")
}

generator dbml {
  provider = "prisma-dbml-generator"
  output   = "../docs/db/generated/dbml"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
